<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         metadata-complete="false">

    <servlet>
        <servlet-name>proxy</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.RawProxyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>proxy</servlet-name>
        <url-pattern>/proxy</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>instapaper</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.InstapaperServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>instapaper</servlet-name>
        <url-pattern>/instapaper</url-pattern>
    </servlet-mapping>

    <!--<servlet>
        <servlet-name>readability</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.ReadabilityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>readability</servlet-name>
        <url-pattern>/readability</url-pattern>
    </servlet-mapping>-->

    <servlet>
        <servlet-name>rss</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.RssServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rss</servlet-name>
        <url-pattern>/rss</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>freake</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.FreakeRssServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>freake</servlet-name>
        <url-pattern>/freake</url-pattern>
    </servlet-mapping>
</web-app>
