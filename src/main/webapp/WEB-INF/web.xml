<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         metadata-complete="false">

    <servlet>
        <servlet-name>proxy</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.ContinuousProxyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>proxy</servlet-name>
        <url-pattern>/proxy</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>instapaper</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.InstapaperServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>instapaper</servlet-name>
        <url-pattern>/instapaper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>mercury</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.MercuryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mercury</servlet-name>
        <url-pattern>/mercury</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rss</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.RssDescriptionExtendingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rss</servlet-name>
        <url-pattern>/rss</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>championat</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.ChampionatServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>championat</servlet-name>
        <url-pattern>/championat</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>twitter</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.TwitterHomeTimelineServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>twitter</servlet-name>
        <url-pattern>/twitter</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>reading_list</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.TwitterReadingListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reading_list</servlet-name>
        <url-pattern>/reading_list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>instagram</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.InstagramServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>instagram</servlet-name>
        <url-pattern>/instagram</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>vk</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.VkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>vk</servlet-name>
        <url-pattern>/vk</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>logs</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.LogsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logs</servlet-name>
        <url-pattern>/logs</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>facebook</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.FacebookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>facebook</servlet-name>
        <url-pattern>/facebook</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>asot</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.AsotServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>asot</servlet-name>
        <url-pattern>/asot</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>banki</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.BankiRuServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>banki</servlet-name>
        <url-pattern>/banki</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>zenit</servlet-name>
        <servlet-class>com.rhcloud.igorbotian.rsskit.servlet.ZenitWebCalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zenit</servlet-name>
        <url-pattern>/zenit</url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>log4jConfiguration</param-name>
        <param-value>/WEB-INF/log4j.xml</param-value>
    </context-param>

    <listener>
        <listener-class>com.rhcloud.igorbotian.rsskit.servlet.RsskitContextListener</listener-class>
    </listener>
</web-app>
